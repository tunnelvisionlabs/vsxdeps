<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>VSSDK.Debugger.Interop.11</id>
    <version>0.0.0</version>
    <authors>Sam Harwell</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      This package provides the Visual Studio "Debugger" COM-interop reference assemblies used by Visual Studio 2012 and newer.

      Microsoft.VisualStudio.Debugger.Interop.11.0.dll
    </description>
    <summary>Visual Studio (Debugger Interop) for Visual Studio 2012+</summary>
    <tags>vssdk vs2012 vs2013</tags>
    <dependencies>
      <!-- Visual Studio 2012 -->
      <dependency id="VSSDK.IDE.11" version="[11.0.2,12.0.0)"/>
      <!--
      Exposed Dependencies:
        Microsoft.VisualStudio.Debugger.InteropA
      -->
      <dependency id="VSSDK.Debugger.Interop.9" version="[9.0.2,10.0.0)"/>

      <!-- Reference previous versions of the Debugger Interop assemblies -->
      <dependency id="VSSDK.Debugger.Interop.10" version="[10.0.2,11.0.0)"/>
    </dependencies>
  </metadata>
  <files>
    <!-- Tools -->

    <file src="install.ps1" target="tools"/>

    <!-- Reference assemblies -->

    <file src="$VSSDK$\VisualStudioIntegration\Common\Assemblies\v2.0\Microsoft.VisualStudio.Debugger.Interop.11.0.dll" target="lib\net40"/>

    <!-- Missing for some reason -->
    <!--<file src="$VSSDK$\VisualStudioIntegration\Common\Assemblies\v2.0\Microsoft.VisualStudio.Debugger.Interop.11.0.xml" target="lib\net40"/>-->
  </files>
</package>
