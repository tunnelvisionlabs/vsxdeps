<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>VSSDK.Shell.Interop</id>
    <version>0.0.0</version>
    <authors>Sam Harwell</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      This package provides the Visual Studio "Shell" COM-interop assemblies used by Visual Studio 2003 and newer.

      Microsoft.VisualStudio.Shell.Interop.dll
    </description>
    <summary>Visual Studio (Shell Interop) for Visual Studio 2003+</summary>
    <tags>vssdk vs2003 vs2005 vs2008 vs2010 vs2012 vs2013 vs2015</tags>
    <dependencies>
      <!-- Visual Studio Base -->
      <dependency id="VSSDK.IDE" version="[7.0.4,8.0.0)"/>
      <!--
      Exposed Dependencies (Microsoft.VisualStudio.Shell.Interop.dll):
        Microsoft.VisualStudio.OLE.Interop
        Microsoft.VisualStudio.TextManager.Interop
      -->
      <dependency id="VSSDK.OLE.Interop" version="[7.0.4,8.0.0)"/>
      <dependency id="VSSDK.TextManager.Interop" version="[7.0.4,8.0.0)"/>
    </dependencies>
  </metadata>
  <files>
    <!-- Tools -->

    <file src="install.ps1" target="tools"/>

    <!-- Reference assemblies -->

    <file src="$VSSDK$\VisualStudioIntegration\Common\Assemblies\v2.0\Microsoft.VisualStudio.Shell.Interop.dll" target="lib\net20"/>
    <file src="$VSSDK$\VisualStudioIntegration\Common\Assemblies\v2.0\Microsoft.VisualStudio.Shell.Interop.xml" target="lib\net20"/>
  </files>
</package>
