<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>VSSDK.Shell.Immutable.12</id>
    <version>0.0.0</version>
    <authors>Sam Harwell</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      This package provides the Visual Studio "Shell Immutable" reference assembly used by Visual Studio 2013 and newer.
    </description>
    <summary>Visual Studio (Shell Immutable) for Visual Studio 2013+</summary>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="PresentationCore" targetFramework="net40"/>
    </frameworkAssemblies>
    <dependencies>
      <!-- Visual Studio 2013 -->
      <dependency id="VSSDK.IDE.12" version="[12.0.0,13.0.0)"/>
      <!--
      Exposed Dependencies (Microsoft.VisualStudio.Shell.Immutable.12.0.dll):
        PresentationCore
      -->
    </dependencies>
  </metadata>
  <files>
    <!-- Tools -->

    <file src="install.ps1" target="tools"/>

    <!-- Reference assemblies -->

    <file src="$VSSDK$\VisualStudioIntegration\Common\Assemblies\v4.0\Microsoft.VisualStudio.Shell.Immutable.12.0.dll" target="lib\net45"/>
    <file src="$VSSDK$\VisualStudioIntegration\Common\Assemblies\v4.0\Microsoft.VisualStudio.Shell.Immutable.12.0.xml" target="lib\net45"/>
  </files>
</package>
