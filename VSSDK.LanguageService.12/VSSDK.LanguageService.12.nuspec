<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>VSSDK.LanguageService.12</id>
    <version>0.0.0</version>
    <authors>Sam Harwell</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      This package provides the Visual Studio "MPF Language Service" reference assembly used by Visual Studio 2013 and newer.

      Microsoft.VisualStudio.Package.LanguageService.12.0.dll
    </description>
    <summary>Visual Studio (MPF Language Service) for Visual Studio 2013+</summary>
    <tags>vssdk vs2013</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="Microsoft.MSXML" targetFramework="net40"/>
      <frameworkAssembly assemblyName="System" targetFramework="net40"/>
      <frameworkAssembly assemblyName="System.Drawing" targetFramework="net40"/>
      <frameworkAssembly assemblyName="System.Windows.Forms" targetFramework="net40"/>
      <frameworkAssembly assemblyName="System.Xml" targetFramework="net40"/>
    </frameworkAssemblies>
    <dependencies>
      <!-- Visual Studio 2013 -->
      <dependency id="VSSDK.IDE.12" version="[12.0.2,13.0.0)"/>
      <!--
      Exposed Dependencies (Microsoft.VisualStudio.Package.LanguageService.12.0.dll):
        Microsoft.MSXML
        Microsoft.VisualStudio.OLE.Interop
        Microsoft.VisualStudio.Shell.12.0
        Microsoft.VisualStudio.Shell.Interop
        Microsoft.VisualStudio.TextManager.Interop
        Microsoft.VisualStudio.TextManager.Interop.8.0
        System
        System.Drawing
        System.Windows.Forms
        System.Xml
      -->
      <dependency id="VSSDK.OLE.Interop" version="[7.0.2,8.0.0)"/>
      <dependency id="VSSDK.Shell.Interop" version="[7.0.2,8.0.0)"/>
      <dependency id="VSSDK.TextManager.Interop" version="[7.0.2,8.0.0)"/>
      <dependency id="VSSDK.TextManager.Interop.8" version="[8.0.2,9.0.0)"/>
      <dependency id="VSSDK.Shell.12" version="[12.0.2,13.0.0)"/>
    </dependencies>
  </metadata>
  <files>
    <!-- Tools -->

    <file src="install.ps1" target="tools"/>

    <!-- Reference assemblies -->

    <file src="$VSSDK$\VisualStudioIntegration\Common\Assemblies\v4.0\Microsoft.VisualStudio.Package.LanguageService.12.0.dll" target="lib\net45"/>
    <file src="$VSSDK$\VisualStudioIntegration\Common\Assemblies\v4.0\Microsoft.VisualStudio.Package.LanguageService.12.0.xml" target="lib\net45"/>
  </files>
</package>
